<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.zackratos.rvitemam">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.zackratos.rvitemam.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.zackratos.rvitemam.sample1.Sample1Activity"></activity>
        <activity android:name="com.github.zackratos.rvitemam.sample3.Sample3Activity" />
        <activity android:name="com.github.zackratos.rvitemam.sample2.Sample2Activity" />
        <activity android:name=".sample4.Sample4Activity" />
        <activity android:name=".sample4.Sample4Activity" />
    </application>

</manifest>